<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>CFI | Contact</title>
    <% include header.ejs %>
  </head>
  <body>
    <% include menuNav.ejs %>
    <form class="form-inline">
            <div class="form-group mx-sm-3 mb-2" style="margin-top:5px;">
              <input type="text" class="form-control" id="us_mn" placeholder="username">
            </div>
            <button type="submit" class="btn mb-2" style="background-color:#63a85a;color:white;">change</button>
          </form>
    <div class="container" style="font-size:30px;font-weight:800;">
        <div class="row">
            <div class="col-md-12">
                <div class="panel panel-primary" style="border-top:7px solid #63a85a;border-top-left-radius: 10px;border-top-right-radius: 10px;background-color: #eff1f4;">
                    <div class="panel-heading" style="background-color: #63a85a;color:white;">
                        <span style="margin: 5px;font-size: 20px;">CFI-Iset Rades</span> 
                        <span style="margin-left:30%;font-size: 20px;">username</span> 
                    </div>
                    <div class="panel-body" style="margin:8px;">
                        <ul class="chat">
                            <li class="left clearfix"><span class="chat-img pull-left">
                            </span>
                                <div class="chat-body clearfix">
                                    <div class="header">
                                        <strong class="primary-font">Oussema Rifi</strong> <small class="pull-right text-muted">
                                    </div>
                                    <p>
                                        Lorem ipsum dolor sit amet, consectetur adipiscing elit. Curabitur bibendum ornare
                                        dolor, quis ullamcorper ligula sodales.
                                    </p>
                                </div>
                            </li>
                            <li class="right clearfix"><span class="chat-img pull-right">
                            </span>
                                <div class="chat-body clearfix">
                                    <div class="header">
                                        <strong class="primary-font" style="color:black;">CFI-Iset Rades</strong>
                                    </div>
                                    <p>
                                        Lorem ipsum dolor sit amet, consectetur adipiscing elit. Curabitur bibendum ornare
                                        dolor, quis ullamcorper ligula sodales.
                                    </p>
                                </div>
                            </li>
                        </ul>
                    </div>
                    <div class="panel-footer" style="margin: 5px;margin-bottom: 0;">
                        <form action="" id="chat_fr">
                        <div class="input-group">
                            <input id="chat_t" type="text" class="form-control input-sm" placeholder="Type your message here..." autocomplete="off"/>
                             <span class="input-group-btn">
                                <button class="btn btn-warning btn-sm" id="btn-chat"  style="padding:8px;margin-bottom:15px;background-color: #63a85a;color:white;">
                                    Send</button>
                             </span>
                        </div>
                    </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <% include footer.ejs %>
  </body>
</html>
